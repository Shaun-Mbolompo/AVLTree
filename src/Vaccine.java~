

public class Vaccine{

    private String[] data = new String[9919];
    int count = 0;
     
    public Vaccine(String dat)
    {	
        String[] split = null;
        String empty_Vac = "<Not Found>";
        try{
            split = dat.split(",");
            data[count] = split[0]+split[1]+split[2];
            count++;
        }
        catch(ArrayIndexOutOfBoundsException e)
        {
            data[count] = split[0]+split[1]+empty_Vac;
        }
    }        
        
        
}